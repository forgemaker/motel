<article class="module width_full">
<div style="width:97%;margin:0 auto;">
    <div class="menu">
        <a class="btn btn-primary search_websync" data-form="#websync_user_list_form" data-model="websync" style="color: white;"><i class="icon-ok icon-white"></i> 搜尋列表</a>
        <a class="btn btn-primary check_all" data-model="websync_user" style="color: white;"><i class="icon-ok icon-white"></i> 全選列表</a>
        <a class="btn btn-primary" data-model="websync_user" href="#!/websync/add" style="color: white;"><i class="icon-edit icon-white"></i> 新增帳號</a>
        <a class="btn btn-danger delete_all" data-form="#websync_user_list_form" data-model="websync_user" href="#" style="color: white;"><i class="icon-trash icon-white"></i> 刪除帳號</a>
    </div>
    <ul class="pager">
        {{#previous_show}}
        <li class="previous">
            <a href="#!/websync/list/1" style="margin-right: 10px;">&larr; 第一頁</a>
        </li>
        <li class="previous">
            <a href="#!/websync/list/{{previous_page}}">&larr; 上一頁</a>
        </li>
        {{/previous_show}}
        {{#next_show}}
        <li class="next">
            <a href="#!/websync/list/{{next_page}}">下一頁 &rarr;</a>
        </li>
        {{/next_show}}
    </ul>
    <form action="#" method="post" id="websync_user_list_form">
    <table id="websync_user_list" class="table table-striped table-bordered table-condensed tablesorter">
        <thead>
            <tr>
                <th>&nbsp;</th>
                <th>編號</th>
                <th>WebSync ID</th>
                <th>建立日期</th>
                <th>最後登入</th>
                <th>自動產生</th>
                <th>動作</th>
            </tr>
            <tr>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                <td><input type="text" name="websync_id" class="span2" placeholder="search id" /></td>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
            </tr>
        </thead>
        <tbody>
        {{#items}}
        <tr>
            <td><input type="checkbox" name="id[]" value="{{id}}"></td>
            <td>{{id}}</td>
            <td>{{websync_id}}</td>
            <td>{{date_time created_on}}</td>
            <td>{{date_time last_login}}</td>
            <td>{{auto_create}}</td>
            <td>
                <a class="btn btn-primary" data-model="websync_user" data-id="{{id}}" href="#!/websync/edit/{{id}}" style="color: white;"><i class="icon-edit icon-white"></i> 編輯</a>&nbsp;
                <a class="btn btn-danger delete" data-model="websync_user" data-id="{{id}}" style="color: white;"><i class="icon-trash icon-white"></i> 刪除</a>
            </td>
        </tr>
        {{/items}}
        </tbody>
    </table>
    </form>
</div>
</article>
